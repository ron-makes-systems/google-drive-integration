{
  "name": "chargebee-connector",
  "version": "1.0.0",
  "repository": "git@gitlab.com:fibery/chargebee-connector.git",
  "main": "src/index.ts",
  "author": "Fibery",
  "private": true,
  "module": "index.js",
  "engines": {
    "node": "^22.5.1"
  },
  "type": "module",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc",
    "postbuild": "cp -R ./src/public build",
    "prestart": "pnpm clean && pnpm build",
    "start": "node ./build/index.js",
    "lint": "eslint . --max-warnings=0 --quiet",
    "prepare": "husky install",
    "pre-commit": "pnpm lint-staged",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fibery/correlation-id": "4.3.0",
    "@fibery/vizydrop-logger": "3.4.0",
    "axios": "^1.7.7",
    "body-parser": "1.20.3",
    "chargebee": "2.43.0",
    "dotenv": "^16.4.5",
    "envalid": "8.0.0",
    "express": "4.21.1",
    "lodash": "^4.17.21",
    "p-limit": "5.0.0",
    "rimraf": "6.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/express": "4.17.21",
    "@types/lodash": "^4.17.12",
    "@types/node": "18.18.12",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "8.54.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vitest": "^0.5.4",
    "got": "^14.6.5",
    "hooks": "0.3.2",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "nock": "^14.0.10",
    "prettier": "3.1.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "src/**/*.*": "prettier src --config .prettierrc.json --write"
  }
}
